{
  "name": "@courier-health/release-please-deno",
  "version": "1.0.1",
  "exports": "./src/mod.ts",
  "tasks": {
    "dev": "deno test --watch",
    "test": "deno test --allow-read --allow-env",
    "pre-commit": "deno check && deno lint && deno task test && deno fmt --check",
    "build:npm": "deno run -A scripts/build_npm.ts"
  },
  "imports": {
    "@deno/dnt": "jsr:@deno/dnt@^0.42.1",
    "@google-automations/git-file-utils": "npm:@google-automations/git-file-utils@^3.0.0",
    "@std/assert": "jsr:@std/assert@1",
    "@std/testing": "jsr:@std/testing@^1.0.14",
    "release-please": "npm:release-please@^17.1.1"
  },
  "fmt": {
    "indentWidth": 2,
    "lineWidth": 120,
    "useTabs": false,
    "singleQuote": true,
    "semiColons": false,
    "proseWrap": "preserve"
  },
  "publish": {
    "exclude": [
      ".envrc",
      "dist/",
      "flake.*",
      "scripts/",
      "test/"
    ]
  }
}
